  "author": "Hunter Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-final/vue-final-modal/issues"
  },
  "homepage": "https://vue-final-modal.org",
  "keywords": [
    "vue",
    "nuxt",
    "modal",
    "dialog",
    "vue-final-modal",
    "vue-modal",
    "vue-dialog",
    "vue-component"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
