{
  "name": "vue-final-modal",
  "version": "3.1.0",
  "description": "Vue Final Modal is a renderless, stackable, detachable and lightweight modal component.",
  "private": false,
  "main": "dist/VueFinalModal.umd.js",
  "module": "dist/VueFinalModal.esm.js",
  "jsnext:main": "dist/VueFinalModal.esm.js",
  "unpkg": "dist/VueFinalModal.umd.js",
  "jsdelivr": "dist/VueFinalModal.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "dist",
    "types",
    "vetur"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "coveralls": "vue-cli-service test:unit && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.6",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.2.0",
    "lint-staged": "^10.2.11",
    "lodash.isequal": "^4.5.0",
    "rollup": "^2.30.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.7",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
